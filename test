local!applicationAndHipMapData: a!forEach(
    items: local!applicationAndAlipEditableDetails,
    expression: a!map(
      fieldName: fv!item.fieldName,
      cardChoices: {
        a!map(
          id: "application/" & lower(fv!item.fieldName),
          primaryText: fv!item.applicationValue
        ),
        a!map(
          id: "slip_entity/" & lower(fv!item.fieldName),
          primaryText: fv!item.alipValue
        )
      },
      selectedChoice: "",
      verificationIsNeeded: fv!item.verificationIsNeeded
    )
  )
