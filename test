a!dropdownField(
  label: "Residence State",
  choiceLabels: a!forEach(
    items: pv!residenceStates,
    expression: if(
      len(trim(fv!item)) > 0,
      fv!item,
      {}
    )
  ),
  choiceValues: a!forEach(
    items: pv!residenceStates,
    expression: if(
      len(trim(fv!item)) > 0,
      fv!item,
      {}
    )
  ),
  value: ri!selectedState,
  saveInto: ri!selectedState
)
